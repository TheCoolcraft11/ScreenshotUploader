    "main": [
      "de.thecoolcraft11.ScreenshotUploader"
    ],
    "client": [
      "de.thecoolcraft11.ScreenshotUploaderClient"
    ],
    "server": [
      "de.thecoolcraft11.ScreenshotUploaderServer"
    ],
    "modmenu": [
      "de.thecoolcraft11.compat.ModMenuCompat"
    ]
