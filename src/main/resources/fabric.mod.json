{
  "schemaVersion": 1,
  "id": "screenshot-uploader",
  "version": "${version}",
  "name": "Screenshot Uploader",
  "description": "Uploads your screenshot to a server so you can show it your friends!",
  "authors": [
    "TheCoolcraft11"
  ],
  "contact": {
    "homepage": "https://modrinth.com/mod/screenshot-uploader/",
    "sources": "https://github.com/TheCoolcraft11/ScreenshotUploader",
