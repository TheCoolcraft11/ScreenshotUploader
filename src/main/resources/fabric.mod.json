	"description": "This mods uploads your screenshots to a server! ",
	"authors": [
		"TheCoolcraft11"
	],
	"contact": {
		"homepage": "https://modrinth.com/mod/screenshot-uploader/",
		"sources": "https://github.com/TheCoolcraft11/ScreenshotUploader",
		"issues": "https://github.com/TheCoolcraft11/ScreenshotUploader/issues"
